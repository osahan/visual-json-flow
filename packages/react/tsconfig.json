{ 
  "compilerOptions": { 
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": false,
    "skipLibCheck": true,
    "declaration": true,
    "jsx": "react-jsx",
    "outDir": "dist",
    "noImplicitAny": false,
    "strictNullChecks": false
  }, 
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
