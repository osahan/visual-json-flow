    "typecheck": "tsc -b",
    "clean": "git clean -xfd -e node_modules",
    "deploy-demo": "./scripts/deploy-demo.sh",
    "preview": "yarn workspace visual-json-flow-demo preview"
  },
  "devDependencies": {
    "typescript": "^5.6.2"
  }
}
