{
  "name": "visual-json-flow",
  "private": true,
  "packageManager": "yarn@4.0.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "yarn workspace @vjf/core build && yarn workspace @vjf/react build && yarn workspace visual-json-flow-demo build",
